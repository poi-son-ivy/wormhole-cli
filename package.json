{
  "name": "wormhole-cli",
  "version": "1.0.0",
  "description": "Command line utilities for Wormhole, interacting with both Ethereum and Solana.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "wormhole-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --no-warnings=ExperimentalWarning dist/index.js",
    "dev": "tsx src/index.ts",
    "test:eth": "npx hardhat test",
    "compile:eth": "npx hardhat compile",
    "deploy:eth": "npx hardhat run --config hardhat.config.ts scripts/deploy.ts --network sepolia",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poi-son-ivy/wormhole-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/poi-son-ivy/wormhole-cli/issues"
  },
  "homepage": "https://github.com/poi-son-ivy/wormhole-cli#readme",
  "dependencies": {
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "hardhat-ethers": "^1.0.1",
    "hardhat-waffle": "^0.0.1-security",
    "inquirer": "^12.10.0",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@types/inquirer": "^9.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.8.1",
    "chai": "^6.2.0",
    "ethers": "^6.9.0",
    "hardhat": "^3.0.7",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
